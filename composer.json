{
    "name": "lucianotonet/laravel-telescope-mcp",
    "version": "0.1.1",
    "description": "MCP Server extension for Laravel Telescope",
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano Tonet",
            "email": "tonetlds@gmail.com",
            "homepage": "https://luciano.tonet.dev",
            "role": "Developer"
        }
    ],
    "keywords": [
        "laravel",
        "telescope",
        "mcp",
        "developer-tools",
        "debugging",
        "monitoring",
        "cursor"
    ],
    "support": {
        "issues": "https://github.com/lucianotonet/laravel-telescope-mcp/issues",
        "source": "https://github.com/lucianotonet/laravel-telescope-mcp"
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "laravel/telescope": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LucianoTonet\\TelescopeMcp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucianoTonet\\TelescopeMcp\\TelescopeMcpServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 